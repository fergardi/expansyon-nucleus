{
  "name": "Expansyon",
  "description": "Expansyon",
  "author": "@fergardi",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p [ browserify-hmr -p 12345 -u localhost:12345 ] -e src/main.js -o dist/build.js --debug --verbose -t [ eslintify --extension js --extension vue --formatter stylish --quiet-ignored ]",
    "serve": "http-server -p 23456 -o -s -c 1",
    "dev": "npm-run-all --parallel watchify serve",
    "test": "karma start karma.config.js",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js",
    "cordova": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > ../cordova/www/js/build.js && cp -R img/* ../cordova/www/img/ && cp -R fonts/* ../cordova/www/fonts/ && cp -R css/* ../cordova/www/css/ && cp node_modules/vue-material/dist/vue-material.css ../cordova/www/css/"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "vue": "^2.2.6",
    "vue-chartjs": "^2.5.6",
    "vue-i18n": "^5.0.3",
    "vue-loader": "^11.3.4",
    "vue-material": "*",
    "vue-router": "^2.3.1",
    "vue-socket.io": "^2.1.1-a",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.13.0",
    "axis": "^1.0.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "envify": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "eslint-plugin-vue": "^2.0.1",
    "eslintify": "^3.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-browserify": "^5.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.30",
    "npm-run-all": "^4.0.2",
    "proxyquireify": "^3.2.1",
    "pug": "^2.0.0-beta6",
    "pug-lint": "^2.4.0",
    "pug-lint-vue": "^0.1.2",
    "stylus": "^0.54.5",
    "uglify-js": "^2.8.21",
    "uglifycss": "^0.0.25",
    "vue-template-compiler": "^2.2.6",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
