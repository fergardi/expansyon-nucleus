// Place your settings in this file to overwrite default and user settings.
{
  "editor.minimap.enabled": true,
  "eslint.enable": true,
  "workbench.colorTheme": "Monokai",
  "files.associations": {
    "*.vue": "vue"
  },
  "eslint.options": {
      "extensions": [".js", ".vue"]
  },
  "eslint.validate": [
      "javascript",
      "html",
      "vue",
      "pug",
      "stylus"
  ],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.wordWrap": "on",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.nyc_output": true,
    "**/coverage": true
  },
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "workbench.iconTheme": "vscode-icons"
}